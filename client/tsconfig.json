{
    "compilerOptions": {
      "paths": {
        "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
      },
      "esModuleInterop": true,
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
        "dom",
        "esnext"
      ],
      "strict": true,
      "sourceMap": true,
      "target": "esnext",
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }